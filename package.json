{
  "name": "messapp",
  "version": "1.0.0",
  "description": "App for posting messages and checking if they are palindromes",
  "main": "index.js",
  "scripts": {
    "start": "set DEBUG=express:* & node server.js",
    "test": "mocha test/unit --reporter spec --recursive",
    "system-test": "mocha test/system --reporter spec --recursive",
    "lint": "eslint lib/** controllers/** models/**",
    "test-suite": "npm run test & npm run system-test",
    "jsdoc": "jsdoc lib/palindrome.js",
    "watch": "watch \"npm run browserify\" public/js",
    "browserify": "browserify public/js/app.jsx -t babelify -o public/out/app.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "Mustafa Nawabi",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.1",
    "body-parser": "^1.17.1",
    "dateformat": "^2.0.0",
    "express": "^4.15.2",
    "jsonschema": "^1.1.1",
    "material-ui": "^0.17.3",
    "mongodb": "^2.2.25",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-tap-event-plugin": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0",
    "npm": "^4.4.4",
    "request": "^2.81.0",
    "watch": "^1.0.2"
  }
}
